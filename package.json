{
  "name": "angular-browser-storage",
  "version": "1.0.16",
  "description": "An observable browser storage service with encryption for Angular",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skrosoft/angular-browser-storage.git"
  },
  "keywords": [
    "angular",
    "browser storage",
    "crypt",
    "encryption",
    "observable"
  ],
  "author": "Vincent Guyard / @skrosoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skrosoft/angular-browser-storage/issues"
  },
  "homepage": "https://github.com/skrosoft/angular-browser-storage#readme",
  "dependencies": {
    "@angular/core": "~7.2.0",
    "crypto-js": "^3.1.9-1",
    "rxjs": "~6.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/jest": "^24.0.9",
    "@types/node": "~8.9.4",
    "@types/uuid": "^3.4.4",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "files": [
    "dist/**/*"
  ]
}
